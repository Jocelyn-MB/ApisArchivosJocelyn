<%- include("../templates/encabezado") %>
<%- include("../templates/menu") %>
<div class="container p-5 my-5 bg-dark text-white">
    <form action="/identificarUsuario" method="post">
        <div class="card">
            <div class="card-header">
                <h1>LOGIN</h1>
            </div>
            <div class="card-body">
                <div class="form-group">
                    <input class="form-control" type="text" name="usuario" id="usuario" placeholder="Usuario" required>
                </div>
                <div class="form-group">
                    <input class="form-control" type="text" name="password" id="password" placeholder="Password" required>
                </div>
            </div>
            <div class="card-footer">
                <button class="btn btn-primary btn-block" type="submit">Login Now</button>
            </div>
        </div>
    </form>
</div>




